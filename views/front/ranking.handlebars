<title>Ranking </title>
  <link rel="stylesheet" href="css/estilo.css">
<body>
     <button type="button" class="btn btn-warning" > <a id="" href="/escoger" style= "color:white; font-weight: 600;">Regresar al men√∫</a></button>

<div class="ranking-container ">
  <div class="padre row">
      <div class="hijo col-md-2">
      <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:20px;">Ranking</h2>

    </div>
    <div class="hijo col-md-2">
    <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:20px;">Puntos</h2>

  </div>
  <div class="hijo col-md-2">
  <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:20px;">Jugador</h2>

</div>
<div class="hijo col-md-2">
<h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:20px;">Foto</h2>

</div>
  </div>

{{#each objPartida}}

<div class="padre row">
    <div class="hijo col-md-2">
    <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:20px;">{{ranking}}</h2>

  </div>

  <div class="hijo col-md-2">
    <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:30px;">{{puntaje}}</h2>

  </div>
  <div class="hijo col-md-2">
    <h2  class="puntaje" style="font-family:monospace; color:white; font-weight:900; font-size:30px;">{{nombre}}</h2>
  </div>
  <div class="hijo col-md-2">
    <img class="rounded-circle personalImg"src="{{foto}}" alt="foto">
  </div>
</div>

{{/each}}

    </div>



